        .member-card {
            border-radius: 10px;
            box-shadow: 0 6px 18px rgba(120, 120, 120, 0.2);
            cursor: pointer;
            margin: 0 15px;
        }
        .member-card .member-info {
            background-color: #FFFFFF;
            border-top-left-radius: 10px;
            border-top-right-radius: 10px;
            padding: 25px 15px 20px 15px;
            text-align: center;
        }
        .member-card .member-info h5 {
            font-weight: 700;
            margin-bottom: 5px;
            font-size: 18px;
            color: #222222;
        }
        .member-card .member-info span {
            display: block;
            font-size: 13px;
            font-weight: 400;
            color: #AAAAAA;
        }
        .member-card .hr-member-info-dashed {
            margin: 0 30px;
            border-top: 1px dashed #CDCDCD;
        }
        .member-card .member-career {
            background-color: #FFFFFF;
            border-bottom-left-radius: 10px;
            border-bottom-right-radius: 10px;
            padding: 20px 15px 25px 15px;
        }
        .member-card .member-career p {
            color: #656565;
            font-size: 14px;
            font-weight: 300;
            margin-bottom: 8px;
            padding: 0 30px;
        }
        .member-card .member-career p > i {
            float: left;
            margin-right: 8px;
        }
        .member-card .member-career .career-label {
            display: block;
            margin-left: 19px;
        }

        .sg-card {
            background-color: #ffffff;
            border-radius: 10px;
            box-shadow: 0 6px 18px rgba(120, 120, 120, 0.2);
            cursor: pointer;
            margin: 0 15px;
            padding: 25px 35px;
        }

        @media(max-width: 768px) {
            .member-card {
                margin: 0 15px 30px 10px !important;
            }
            .sg-card {
                margin: 0 15px 30px 10px !important;
            }
        }
