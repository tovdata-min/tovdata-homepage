    <style>
        .timeline {
            width: 100%;
        }
        .timeline-wrapper {
            display: flex;
            flex-wrap: wrap;
            flex-direction: row-reverse;
        }
        .timeline-item {
            position: relative;
            width: 50%;
        }
        .timeline-item:nth-child(even) {
            padding-right: 2.5rem;
            padding-bottom: 2rem;
            text-align: right;
        }
        .timeline-item:nth-child(odd) {
            /*margin-left: 50%;*/
            padding-left: 2.5rem;
            padding-bottom: 2rem;
            text-align: left;
        }
        .timeline-item:nth-child(odd)::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 16px;
            height: 16px;
            margin-top: .425rem;
            margin-left: -.5rem;
            border: .0625rem solid #615C8F;
            border-radius: 50%;
        }
        .timeline-item:nth-child(odd)::after {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            bottom: -.1875rem;
            width: .0625rem;
            margin-top: 1.675rem;
            margin-left: -.0325rem;
            background-color: #C1BCEF;
        }
        .timeline-item .timeline-date {
            font-size: 25px;
            font-weight: 600;
            height: 30px;
            margin-bottom: 7px;
        }
        .timeline-item .timeline-item-content {
            font-size: 15px;
            font-weight: 200;
            margin-bottom: 8px;
        }
        .timeline-item .timeline-item-content:last-child {
            margin-bottom: 15px;
        }

        @media (max-width: 768px) {
            .timeline {
                padding: 0 15px;
            }

            .timeline-item:nth-child(odd),
            .timeline-item:nth-child(even) {
                width: 100%;
                margin-left: 0;
                padding-left: 2.5rem;
                padding-right: 0;
                padding-bottom: 2rem;
                text-align: left;
            }

            .timeline-item:nth-child(odd) {
                padding-bottom: 0;
            }
            
            .timeline-item::before {
                left: 0;
                margin-left: -.5rem;
            }
            
            .timeline-item::after {
                content: "";
                position: absolute;
                top: 0;
                left: 0;
                bottom: -.1875rem;
                width: .0625rem;
                /*margin-top: 1.675rem;*/
                margin-left: -.0325rem;
                background-color: #C1BCEF;
            }

            .timeline-item .timeline-date {
                height: auto;
            }

        }
    </style>
</head>
